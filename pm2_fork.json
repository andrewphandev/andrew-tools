{
  "apps": [
    {
      "name": "SystembedController-worker",
      "script": "./www/server.js",
      "autorestart": true,
      // "exec_mode": "cluster",
      // "instances": 0,
      "restart_delay": 1000,
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "merge_logs": true,
      "wait_ready": true,
      "out_file": "./logs/pm2.log",
      "error_file": "./logs/pm2.log",
      "watch": false,
      "ignore_watch": [],
      "max_memory_restart": "1G",
      "env": {
        "NODE_ENV": "production"
      }
    }
  ]
}
